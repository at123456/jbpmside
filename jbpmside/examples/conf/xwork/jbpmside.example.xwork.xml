<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.1.1//EN" "http://www.opensymphony.com/xwork/xwork-1.1.1.dtd">
<xwork>

    <package name="jbpmside" extends="common" namespace="/jbpmside">
        <!--发布所有的流程定义-->
        <action name="deployAll" class="cn.org.jbpmside.demo.action.process.JbpmProcessAction" method="deployAll">
            <interceptor-ref name="basicStack"/>
            <result type="redirect" name="success">/jbpmside/findAllProcessDefination.action</result>
        </action>
        <!--发布指定的流程定义-->
        <action name="deploy" class="cn.org.jbpmside.demo.action.process.JbpmProcessAction" method="deploy">
            <interceptor-ref name="basicStack"/>
            <result type="redirect" name="success">/jbpmside/findAllProcessDefination.action</result>
        </action>
        <!--查询所有的流程定义-->
        <action name="findAllProcessDefination" class="cn.org.jbpmside.demo.action.process.JbpmProcessAction"
                method="findAllProcessDefination">
            <interceptor-ref name="basicStack"/>
            <result type="dispatcher" name="success">/jbpmside/webwork/alllist.jsp</result>
        </action>
        <!--开始一个新的流程-->
        <action name="startProcess" class="cn.org.jbpmside.demo.action.process.JbpmProcessAction"
                method="startProcess">
            <interceptor-ref name="basicStack"/>
            <result type="redirect" name="success">/jbpmside/todoList.action</result>
        </action>

        <!--获取待签列表-->
        <action name="todosign" class="cn.org.jbpmside.demo.action.task.JbpmTaskAction"
                method="todosign">
            <interceptor-ref name="basicStack"/>
            <result type="dispatcher" name="success">/jbpmside/webwork/todosign.jsp</result>
        </action>
        <!--获取已签列表-->
        <action name="alreadysign" class="cn.org.jbpmside.demo.action.task.JbpmTaskAction"
                method="alreadysign">
            <interceptor-ref name="basicStack"/>
            <result type="dispatcher" name="success">/jbpmside/webwork/alreadysign.jsp</result>
        </action>
        <!--获取待办列表-->
        <action name="todoList" class="cn.org.jbpmside.demo.action.task.JbpmTaskAction"
                method="todoList">
            <interceptor-ref name="basicStack"/>
            <result type="dispatcher" name="success">/jbpmside/webwork/todolist.jsp</result>
        </action>
        <!--获取已办列表-->
        <action name="completedList" class="cn.org.jbpmside.demo.action.task.JbpmTaskAction"
                method="completedList">
            <interceptor-ref name="basicStack"/>
            <result type="dispatcher" name="success">/jbpmside/webwork/completedlist.jsp</result>
        </action>
        <!-- 获取办结列表-->
        <action name="processCompletedList" class="cn.org.jbpmside.demo.action.task.JbpmTaskAction"
                method="processCompletedList">
            <interceptor-ref name="basicStack"/>
            <result type="dispatcher" name="success">/jbpmside/webwork/processcomplete.jsp</result>
        </action>

        <!--获取流程跟踪列表-->
        <action name="processDetailList" class="cn.org.jbpmside.demo.action.task.JbpmTaskAction"
                method="processDetailList">
            <interceptor-ref name="basicStack"/>
            <result type="dispatcher" name="success">/jbpmside/webwork/detail.jsp</result>
        </action>
        <!--欢迎页-->
        <action name="welcome" class="cn.org.jbpmside.demo.action.JbpmWelcomeAction">
            <interceptor-ref name="basicStack"/>
            <result type="dispatcher" name="success">/jbpmside/navigation.jsp</result>
        </action>
        <!--完成某个任务-->
        <action name="completdTask" class="cn.org.jbpmside.demo.action.task.JbpmTaskAction"
                method="completdTask">
            <interceptor-ref name="basicStack"/>
            <result type="redirect" name="success">/jbpmside/todoList.action</result>
        </action>
        <!--签收某个任务-->
        <action name="signTask" class="cn.org.jbpmside.demo.action.task.JbpmTaskAction"
                method="signTask">
            <interceptor-ref name="basicStack"/>
            <result type="redirect" name="success">/jbpmside/todosign.action</result>
        </action>
        
    </package>
</xwork>