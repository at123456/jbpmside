<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.1.1//EN" "http://www.opensymphony.com/xwork/xwork-1.1.1.dtd">
<xwork>
    <include file="webwork-default.xml"/>

    <package name="common" extends="webwork-default" namespace="/common">
        <global-results>
            <result name="nopermission">/common/nopermission.ftl</result>
            <result name="error">/common/error.ftl</result>
            <result name="exception">/common/exception.ftl</result>
            <result name="withoutcaslogin" type="redirect">/extcomponent/security/login.jsp</result>
            <result name="report" type="dispatcher">/report</result>
            <result name="reportBeanDatasource" type="dispatcher">/reportBeanDatasource</result>
        </global-results>
    </package>
</xwork>